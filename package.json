{
  "name": "dbUser",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "test": "yarn lint && yarn flow && jest",
    "lint": "standard --verbose | snazzy",
    "test:unit": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s",
    "flow": "flow",
    "flow-stop": "flow stop"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@expo/react-native-action-sheet": "^1.1.2",
    "apisauce": "^0.16.0",
    "autobind-decorator": "^2.1.0",
    "axios": "^0.18.0",
    "format-json": "^1.0.3",
    "i18n-js": "^3.0.11",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.22.2",
    "native-base": "^2.8.0",
    "prop-types": "^15.6.1",
    "querystringify": "1.0.0",
    "ramda": "^0.25.0",
    "react": "16.6.3",
    "react-native": "0.62.3",
    "react-native-animatable": "^1.3.0",
    "react-native-communications": "^2.2.1",
    "react-native-config": "^0.10.0",
    "react-native-gesture-handler": "1.1.0",
    "react-native-i18n": "^2.0.15",
    "react-native-lightbox": "^0.8.0",
    "react-native-material-ripple": "0.8.0",
    "react-native-material-textfield": "^0.12.0",
    "react-native-modalbox": "^1.6.0",
    "react-native-paper": "^2.0.1",
    "react-native-parsed-text": "0.0.20",
    "react-native-typescript-material-ui-collection": "^0.0.49",
    "react-native-vector-icons": "6.1.0",
    "react-navigation": "^3.5.1",
    "react-navigation-material-bottom-tabs": "^0.4.0",
    "react-navigation-redux-helpers": "^2.0.6",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-axios-middleware": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0",
    "reduxsauce": "0.7.0",
    "regenerator-runtime": "^0.12.1",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/runtime": "^7.4.2",
    "@react-native-community/eslint-config": "^0.0.3",
    "@storybook/addon-storyshots": "^3.4.0",
    "@storybook/addons": "^3.4.0",
    "@storybook/channels": "^3.4.0",
    "@storybook/react-native": "^3.4.0",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.12.0",
    "@types/react": "16.7.7",
    "@types/react-native": "0.57.13",
    "@types/react-navigation": "2.13.5",
    "@types/react-test-renderer": "16.0.3",
    "@types/webpack": "^4.4.17",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "^7.1.1",
    "babel-jest": "23.6.0",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.15.3",
    "husky": "^0.14.3",
    "ignite-animatable": "^1.0.0",
    "ignite-boilerplate-andross-typescript": "^0.1.97",
    "ignite-i18n": "^1.1.1",
    "ignite-redux-persist": "^1.1.0",
    "ignite-standard": "^1.0.0",
    "ignite-vector-icons": "^1.1.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.48.0",
    "mockery": "^2.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.5.2",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.6.3",
    "reactotron-react-native": "^2.1.0",
    "reactotron-redux": "^2.1.0",
    "reactotron-redux-saga": "^2.1.0",
    "schedule": "^0.4.0",
    "snazzy": "^8.0.0",
    "standard": "10.0.2",
    "ts-jest": "^23.10.3",
    "typescript": "^3.1.1"
  },
  "jest": {
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|lottie-react-native|native-base|react-native-vector-icons|react-navigation|react-navigation-redux-helpers)/"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|json)$": "<rootDir>/js/Tests/__mocks__/fileMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/js/Tests/__mocks__/react-native-i18n.mock",
      "<rootDir>/js/Tests/__mocks__/react-native-action-sheet.mock",
      "<rootDir>/js/Tests/Setup/test-shim.js",
      "<rootDir>/js/Tests/Setup/test-setup.js",
      "<rootDir>/js/Tests/Setup/test-preprocessor.js"
    ],
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "cacheDirectory": ".jest/cache"
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
